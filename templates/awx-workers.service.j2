# {{ ansible_managed }}

[Unit]
Description=AWX Django Channels Worker Pool
Wants=network-online.target

[Service]
User={{ awx_debian_user }}
Group={{ awx_debian_user }}
ExecStart={{ awx_debian_home }}/venv/awx/bin/awx-manage runworker --only-channels websocket.*
WorkingDirectory={{ awx_debian_home }}

[Install]
WantedBy=multi-user.target
